config dnsmasq 'invizbox'
	option authoritative '1'
	option boguspriv '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	option filterwin2k '0'
	list interface 'loopback'
	option leasefile '/tmp/dhcp.leases.go'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '1'
	option nonegcache '0'
	option nonwildcard '1'
	option readethers '1'
	list rebind_domain 'onion'
	option rebind_localhost '1'
	option rebind_protection '1'
	option resolvfile '/tmp/resolv.conf.auto'
	list server '/onion/172.31.1.1#9053'

config dnsmasq 'vpn'
	option authoritative '1'
	option boguspriv '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	option filterwin2k '0'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option nonegcache '0'
	option nonwildcard '1'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	list rebind_domain '/.netflix.com/.netflix.net/.nflxvideo.net/.nflxext.com/.nflxso.net/.nflximg.net/'
	option rebind_localhost '1'  # enable for RBL checking and similar services
	option rebind_protection '1'  # disable if upstream must serve RFC1918 addresses
	list server '8.8.8.8@tun0'
	list server '8.8.4.4@tun0'

config dnsmasq 'tor'
	option authoritative '1'
	option boguspriv '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	option filterwin2k '0'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option nonegcache '0'
	option nonwildcard '1'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	list rebind_domain 'onion'
	option rebind_localhost '1'  # enable for RBL checking and similar services
	option rebind_protection '1'  # disable if upstream must serve RFC1918 addresses
	list server '172.31.1.1#9053'

config dnsmasq 'auto'
	option authoritative '1'
	option boguspriv '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '1'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	option filterwin2k '0'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option nonegcache '0'
	option nonwildcard '1'
	list notinterface 'loopback'
	option readethers '1'
	option rebind_localhost '1'  # enable for RBL checking and similar services
	option rebind_protection '0'  # disable if upstream must serve RFC1918 addresses
	option resolvfile '/tmp/resolv.conf.auto'

config dnsmasq 'captive'
	list address '/#/10.101.0.1'
	option authoritative '1'
	option boguspriv '1'
	option dhcpscript '/usr/lib/lua/new_device.lua'
	option disabled '0'
	option domain 'lan'
	option domainneeded '1'
	option expandhosts '1'
	option filterwin2k '0'
	list interface 'lan'
	option leasefile '/tmp/dhcp.leases'
	option local '/lan/'
	option localise_queries '1'
	option localservice '1'
	option localuse '0'
	option nonegcache '0'
	option nonwildcard '1'
	option noresolv '1'
	list notinterface 'loopback'
	option readethers '1'
	option rebind_localhost '1'  # enable for RBL checking and similar services
	option rebind_protection '1'  # disable if upstream must serve RFC1918 addresses

config dhcp lan
	option interface 'lan'
	option leasetime '10m'
	option limit 251
	option start '1'
