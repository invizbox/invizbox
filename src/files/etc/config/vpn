config active 'active'
	option expiry 'unknown'
	option mode 'vpn'
	option registered 'unknown'
	option renewal 'unknown'
	option vpn 'auckland'

config server 'auckland'
	option address '10.10.10.10'
	option city 'Auckland'
	option country 'NZ'
	option name 'auckland'
	option template '/etc/openvpn/templates/my.ovpn.template'

config server 'amsterdam'
	option city 'Amsterdam'
	option country 'NL'
	option filename '/etc/openvpn/files/my.ovpn.file'
	option name 'amsterdam'
